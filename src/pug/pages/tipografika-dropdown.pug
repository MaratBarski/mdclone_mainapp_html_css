extends ../layout/main

block variables
	- var title = 'Typography';
	- var h1 = 'Dropdown';

block content
	section
		.container-fluid
			.row
				.col-12

					.h2 Dropdown with auto autocomplete
					.drop-auto
						.drop-auto__head
							.drop-auto__search
								input(type="text", value="dia", placeholder="Type or Select Property...",)
								span.drop-auto__search-close
									+svg('ic-close-small','ic-close-small')
							.drop-auto__head-arr
								+svg('ic-select-arr-down','ic-select-arr-down')
						.drop-auto__popup
							.drop-auto__option
								.drop-auto__param <span class="drop-auto__accent">dia</span>cgnosis - I - ageatevent
								.drop-auto__param <span class="drop-auto__accent">dia</span>gnosis - I - startdate (default date)
								.drop-auto__param <span class="drop-auto__accent">dia</span>gnosis - II - ageatevent
								.drop-auto__param <span class="drop-auto__accent">dia</span>gnosis - I - insert_date
								.drop-auto__param <span class="drop-auto__accent">dia</span>gnosis - II - insert_date
								.drop-auto__param <span class="drop-auto__accent">dia</span>gnosis - I - conditioncode
								.drop-auto__param <span class="drop-auto__accent">dia</span>gnosis - I - conditioncodedesc
								.drop-auto__param <span class="drop-auto__accent">dia</span>gnosis - I - internalpatientid
								.drop-auto__param <span class="drop-auto__accent">dia</span>gnosis - I - sequencenumber
								.drop-auto__param <span class="drop-auto__accent">dia</span>gnosis - II - conditioncode
								.drop-auto__param <span class="drop-auto__accent">dia</span>gnosis - II - conditioncodedesc
									+svg('ic-failure','ic-failure')
								.drop-auto__param <span class="drop-auto__accent">dia</span>gnosis - II - conditioncodedesc
								
							.drop-auto-info
								.drop-auto-info__desc
									.drop-auto-info__title Description:
									.drop-auto-info__text The service line associated with the admission if available, if the service line is not available the attending provider
									.drop-auto-info__link 
										a(href="#") More details
											+svg('ic-external-link','ic-external-link')

								.drop-auto-info__detail
									.drop-auto-info__prop
										.drop-auto-prop
											.drop-auto-prop__title Populated:
											.drop-auto-prop__val
												.progress.progress_green
													.progress__bg(style='width: 86%')
													.progress__text 86%


										.drop-auto-prop
											.drop-auto-prop__title Data Type:
											.drop-auto-prop__val
												+svg('ic-calendar','ic-calendar')
												span Date

									.drop-auto-info__bottom
										.drop-auto-info__title Data Sample:
										.drop-auto-info__text 28 Mar, 2011
					
					br
					br
					.drop-auto
						.drop-auto__head
							.drop-auto__search
