							.drop-auto__option
								.drop-auto__sect
									.drop-auto__sect-head
										.drop-auto__icon
											+svg('ic-my-events','ic-my-events')
										.drop-auto__title My Events
									.drop-auto__list
										.drop-auto__param <span class="drop-auto__accent">dia</span>gnosis - I - conditioncode
											+svg('ic-failure','ic-failure')
										.drop-auto__param <span class="drop-auto__accent">dia</span>gnosis - I - conditioncodedesc

								.drop-auto__sect
									.drop-auto__sect-head
										.drop-auto__icon
											+svg('ic-diagnosis','ic-diagnosis')
										.drop-auto__title Diagnosis
									.drop-auto__list
										.drop-auto__param <span class="drop-auto__accent">Dia</span>gnosis
										.drop-auto__param <span class="drop-auto__accent">Dia</span>gnosis in admission

								.drop-auto__sect
									.drop-auto__sect-head
										.drop-auto__icon
											+svg('ic-admissions','ic-admissions')
										.drop-auto__title Admissions
									.drop-auto__list
										.drop-auto__param Admissions

								.drop-auto__sect
									.drop-auto__sect-head
										.drop-auto__icon
											+svg('ic-medications','ic-medications')
										.drop-auto__title Medications
									.drop-auto__list
										.drop-auto__param Medications - from home
								
							.drop-auto-info
								.drop-auto-info__desc
									.drop-auto-info__title Description:
									.drop-auto-info__text It is a long established fact that a reader will be distracted by the readable content of a page when looking at its layout.
									

								.drop-auto-info__detail
									.drop-auto-info__prop
										.drop-auto-prop
											.drop-auto-prop__title The available data is since:
											.drop-auto-prop__val
												span.drop-auto-prop__year 1998

									.drop-auto-info__bottom
										.drop-auto-info__title Default Date:
											+svg('ic-info','ic-info')
										.drop-auto-info__text Diagnosis date
					
					br
					br
					.drop-auto
						.drop-auto__head
							.drop-auto__search
								input(type="text", value="dia", placeholder="Type or Select Property...",)
