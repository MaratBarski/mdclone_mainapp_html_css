extends ../layout/main

block variables
	- var title = 'Content';
	- 
		var pagelist = [
			{href: 'main.html',name:'Imported files'},
			{href: 'main-popup.html',name:'Imported files with popup'},
			{href: 'main-modal.html',name:'Imported files with modal'},
			{href: 'main-modal-active.html',name:'Imported files with modal (active and error)'},
			{href: 'tipografika.html',name:'Typography'},
		];

block content
	section
		.container
			.row
				.col-12
					h1 Content
					ul
						each page in pagelist
							li
								a(href=page.href,)= page.name


